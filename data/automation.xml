<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="increment_sample_transport_sequence" model="ir.sequence">
    <field name="name">Sample Transport Incrementation</field>
    <field name="code">increment_sample_transport</field>
    <field name="prefix">ST/%(range_year)s/%(month)s  </field>
    <field name="padding">3</field>
    <field name="company_id" eval="False"/>
</record>
        <record id="increment_result_transport_sequence" model="ir.sequence">
    <field name="name">Result Transport Incrementation</field>
    <field name="code">increment_result_transport</field>
    <field name="prefix">RT/%(range_year)s/%(month)s  </field>
    <field name="padding">3</field>
    <field name="company_id" eval="False"/>
</record>


    </data>
</odoo>